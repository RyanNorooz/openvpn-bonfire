{
  "name": "openvpn-automation",
  "version": "1.0.0",
  "description": "openvpn automation for personal use",
  "author": "Ryan Norooz",
  "scripts": {
    "dev": "node ./src/db/databaseDevSetup.js && next dev",
    "build": "next build",
    "start": "node ./src/db/databaseSetup.js && next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.?(c)[jt]s?(x)": [
      "eslint --fix --ignore-path .prettierignore"
    ],
    "*": [
      "prettier --ignore-unknown --write"
    ]
  },
  "browserslist": [
    "defaults",
    ">1%",
    "not dead"
  ],
  "dependencies": {
    "@dicebear/avatars": "^4.10.2",
    "@dicebear/avatars-identicon-sprites": "^4.10.2",
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.9",
    "@headlessui/react": "^1.6.0",
    "next": "12.1.5",
    "node-fetch": "^3.2.4",
    "nprogress": "^0.2.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "sharp": "^0.30.4",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@iconify/react": "^3.2.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.30",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "pnpm": "^6.32.11",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.4",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
