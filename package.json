{
  "name": "openvpn-automation",
  "version": "1.0.0",
  "description": "openvpn automation for personal use",
  "author": "Ryan Norooz",
  "scripts": {
    "dev": "node ./db/databaseSetup.js && next dev",
    "build": "next build",
    "start": " next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.?(c)[jt]s?(x)": [
      "eslint --fix --ignore-path .prettierignore"
    ],
    "*": [
      "prettier --ignore-unknown --write"
    ]
  },
  "browserslist": [
    "defaults",
    ">1%",
    "not dead"
  ],
  "dependencies": {
    "@dicebear/avatars": "^4.10.2",
    "@dicebear/avatars-identicon-sprites": "^4.10.2",
    "@headlessui/react": "^1.5.0",
    "next": "12.1.0",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sharp": "^0.30.2",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@iconify/react": "^3.1.4",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.21",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "pnpm": "^6.32.2",
    "postcss": "^8.4.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
}
